{
  "name": "thanjavur-talkmate",
  "version": "1.0.0",
  "description": "A specialized AI slang translator for visitors in Thanjavur",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "vercel dev",
    "start": "npm run build && node dist/index.js",
    "setup": "node setup-ai.js",
    "test": "jest",
    "test:properties": "jest tests/properties",
    "type-check": "tsc --noEmit",
    "validate-doc": "node dist/services/DocumentParser.js --validate"
  },
  "keywords": [
    "ai",
    "translation",
    "tamil",
    "thanjavur",
    "cultural"
  ],
  "author": "Thanjavur TalkMate Team",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@vercel/node": "^3.2.29",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "dotenv": "^16.6.1",
    "fast-check": "^3.15.0"
  }
}
